nextflow_pipeline {

    name "Test phasing only workflow"
    script "../main.nf"
    tag "pipeline"

    test("phasing only workflow") {
        options "-stub"

        when {
            params {
                outdir = "$outputDir"
                germline = false
                skip_somatic = true

            }
        }

        then {
            assert workflow.success
        }
    }
}
